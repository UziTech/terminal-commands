{
  "name": "terminal-commands",
  "main": "./lib/terminal-commands",
  "version": "1.3.0",
  "description": "Setup commands to run in the terminal",
  "keywords": [
    "atom",
    "terminal",
    "commands"
  ],
  "repository": "https://github.com/UziTech/terminal-commands",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "atom --test spec",
    "lint": "eslint .",
    "semantic-release": "semantic-release"
  },
  "atomTestRunner": "./spec/runner.js",
  "consumedServices": {
    "runInTerminal": {
      "description": "Allow to run commands in platformio-ide-terminal.",
      "versions": {
        "0.14.5": "consumeRunInTerminal"
      }
    },
    "terminusTerminal": {
      "description": "Allow to run commands in terminus.",
      "versions": {
        "^1.1.1": "consumeTerminusTerminal"
      }
    }
  },
  "dependencies": {
    "fs-plus": "^3.1.1",
    "promisificator": "^4.1.1"
  },
  "devDependencies": {
    "@semantic-release/apm-config": "^6.0.2",
    "atom-jasmine3-test-runner": "^4.3.9",
    "eslint": "^6.6.0",
    "semantic-release": "^15.13.30",
    "temp": "^0.9.1"
  }
}
